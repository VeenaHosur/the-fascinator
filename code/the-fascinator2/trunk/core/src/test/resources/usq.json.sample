{
    "harvester": {
        "type": "oai-pmh",
        "oai-pmh": {
            "url": "http://eprints.usq.edu.au/cgi/oai2",
            "maxRequests": 1
        }
    },
    "transformer": {
        "conveyer": "aperture",
        "aperture": {
            "outputPath": "/tmp/aperture"
        }
    },
    "indexer": {
        "script": {
            "type": "python",
            "rules": "usq.py"
        },
        "params": {
            "repository.name": "USQ",
            "repository.type": "ePrints"
        }
    }
}
