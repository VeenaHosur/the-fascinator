{
    "harvest": {
        "type": "jsonq",
        "jsonq": {
            "url": "http://localhost:9000/"
        }
    },
    
    "transformer": {
        "conveyer": "aperture, ice2",
        "aperture": {
            "outputPath": "${user.home}/aperture"
        },
        "ice2": {
            "url": "http://ice-service.usq.edu.au/api/convert/",
            "outputPath": "${user.home}/ice2-output"
        }
    },
    
        
    "indexer": {
        "script": {
            "type": "python",
            "rules": "local-files.py"
        },
        "params": {
            "repository.name": "Json Queue",
            "repository.type": "Local File System"
        }
    }
}
