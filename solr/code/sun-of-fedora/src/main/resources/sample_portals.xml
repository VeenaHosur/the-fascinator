<?xml version="1.0" encoding="UTF-8"?>
<portals>
  <defaults>
    <records-per-page>10</records-per-page>
    <facet-count>25</facet-count>
    <facet-fields>
      <field name="repository_name">Institution</field>
      <field name="f_creator">Author</field>
      <field name="f_subject">Subject</field>
      <field name="f_type">Type</field>
    </facet-fields>
  </defaults>
  <portal name="usq">
    <description>USQ ePrints</description>
    <query>repository_name:"USQ ePrints"</query>
    <facet-fields>
      <field name="f_creator">Author</field>
      <field name="f_subject">Course</field>
    </facet-fields>
  </portal>
</portals>