<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portal name="default">
    <description>Everything</description>
    <records-per-page>10</records-per-page>
    <facet-count>10</facet-count>
    <facet-fields>
        <user name="repository_name">Repository</user>
        <user name="f_type">Type</user>
        <user name="f_subject">Subject</user>
        <user name="f_creator">Author</user>
    </facet-fields>
    <roles>
        <role id="admin">
            <users/>
            <query>group_access:&quot;guest&quot; OR group_access:&quot;admin&quot;</query>
        </role>
        <role id="guest">
            <users/>
            <query>group_access:&quot;guest&quot;</query>
        </role>
    </roles>
</portal>
